{
  "name": "react-raw-webpack-seed",
  "description": "Minimal React setup with raw Webpack bundler",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --mode=development",
    "build:dev": "cross-env NODE_ENV=development webpack --mode=development",
    "build:ci": "cross-env CI=1 webpack --mode=production",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint ./",
    "clean": "rimraf ./dist ./public ./build"
  },
  "dependencies": {
    "json-server": "^0.14.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "cssnano": "^4.1.7",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.89.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "redbox-react": "^1.6.0",
    "resolve-url-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "browserslist": "defaults"
}
